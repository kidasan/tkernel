/*
 *	rominfo.lnk (config, SH7776)
 *	ROM information linker script
 */

OUTPUT_FORMAT("elf32-shl", "elf32-shl", "elf32-shl")
OUTPUT_ARCH(sh)

PHDRS {
	data	PT_LOAD;
}

SECTIONS {
	/* start address of data section = ROMInfo in rominfo_depend.h */
	.data 0x80010000 : {
		*(.data)
	} :data
}
